{
  "name": "ok-form",
  "version": "0.0.0",
  "description": "minimal js object validation",
  "author": "Ryan Baxley",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "yarn run clean && yarn run build:src && yarn run build:types",
    "build:src": "babel ./src -o ./lib/index.js --ignore=**/__tests__/* --extensions .ts -s",
    "build:types": "tsc --emitDeclarationOnly",
    "ci": "yarn run lint && yarn run test && yarn run size && yarn run build",
    "clean": "rimraf lib/*",
    "explore": "source-map-explorer lib/index.js",
    "lint": "eslint src/**/*.ts --max-warnings=0",
    "prepublish": "yarn run build",
    "release": "np",
    "size": "bundlesize",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc -w --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "bundlesize": "^0.17.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jest": "^22.4.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "np": "^5.0.0",
    "prettier": "1.16.4",
    "rimraf": "^2.6.3",
    "source-map-explorer": "^1.8.0",
    "typescript": "^3.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": "./lib/index.js",
      "maxSize": "5.5 kB"
    }
  ]
}
