{
  "name": "ok-form",
  "version": "0.0.0",
  "description": "",
  "author": "Ryan Baxley",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "babel ./src -d ./lib --ignore=**/__tests__/* --extensions .ts",
    "release": "yarn run clean && yarn run build",
    "typecheck": "tsc -w",
    "lint": "eslint src/**/*.ts --max-warnings=0",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jest": "^22.4.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier": "1.16.4",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
