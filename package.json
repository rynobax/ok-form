{
  "name": "ok-form",
  "version": "1.0.1",
  "description": "minimal js object validation",
  "author": "Ryan Baxley",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/ok-form.esm.js",
  "types": "types/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "validation",
    "forms",
    "error"
  ],
  "scripts": {
    "build": "yarn run clean && yarn run build:src && yarn run build:types && yarn run build:readme",
    "build:readme": "doctoc README.md",
    "build:src": "rollup -c && cp-cli ./index.js ./dist/index.js",
    "build:types": "tsc --emitDeclarationOnly",
    "ci": "yarn run lint && yarn run test && yarn run size && yarn run build",
    "clean": "rimraf dist/*",
    "lint": "eslint src/**/*.ts --max-warnings=0",
    "prepublish": "yarn run build",
    "release": "np",
    "size": "bundlesize",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc -w --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-plugin-transform-async-to-promises": "^0.8.9",
    "bundlesize": "^0.17.1",
    "cp-cli": "^2.0.0",
    "doctoc": "^1.4.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jest": "^22.4.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "np": "^5.0.0",
    "prettier": "1.16.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.10.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-size-snapshot": "^0.8.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript": "^1.0.1",
    "typescript": "^3.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/ok-form.cjs.min.js",
      "maxSize": "3 kB"
    }
  ]
}
